pragma solidity ^0.8.0;


    contract BasicArithmeticOperations {
        uint public x=10;
        uint public y=8;



        uint public  sum = x+y;
        uint public diff =x-y;
        uint public product=x*y;
        uint public quotient=x/y;
        uint public remainder=x%y;


        
    }